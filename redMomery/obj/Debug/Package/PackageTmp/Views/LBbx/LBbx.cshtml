            if (target.dataset.ctaTarget) {
                _gaq.push(['_trackEvent', 'ctajs', 'modal']);
                closeFn = cta(target, document.querySelector(target.dataset.ctaTarget), { relativeToWindow: true }, function showModal(modal2) {
