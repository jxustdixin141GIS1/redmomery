@{
    ViewBag.Title = "ungeofile";
}

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">

<head>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <title>基于HTML5的jquery文件上传插件</title>


    <link href="~/resource/js/html5uploader.css" rel="stylesheet" />
    <script src="~/resource/js/jquery.js"></script>
    <script src="~/resource/js/jquery.html5uploader.js"></script>
    <style type="text/css">
    </style>

    <script type="text/javascript">

$(function(){

	$('#upload').html5uploader({

		auto:false,

		multi:true,

		removeTimeout:9999999,

		url:'./upload/upngeofile',

		onUploadStart:function(){

			alert('开始上传');

			},

		onInit:function(){

			alert('初始化');

			},

		onUploadComplete:function(){

			alert('上传完成');

			}

		});

	});

    </script>

</head>


<body>

    <div id="upload"></div>

</body>

</html>
